@import "../../../bower_components/normalize.css/normalize.css"

*
    -webkit-overflow-scroll: touch
    -webkit-user-select:none
    -khtml-user-select:none
    user-select:none
    -webkit-font-smoothing:antialiased
    -webkit-tap-highlight-color:rgba(0,0,0,0)
    outline:none

.clearfix:after
    visibility: hidden
    display: block
    font-size: 0
    content: " "
    clear: both
    height: 0

.clearfix
    display:inline-table
    height:1%
    display:block

body
    margin:0
    padding:0
    position:absolute
    width:100%
    height:100%
    background-color:#eeeeee

image-auto(image)
    background-image:url('/static/image/'+image)
    background-size:100% 100%
    background-repeat:no-repeat
    background-position:center

image-wauto(image)
    background-image:url('/static/image/'+image)
    background-size:100% auto
    background-repeat:no-repeat
    background-position:center
    
image-hauto(image)
    background-image:url('/static/image/'+image)
    background-size:auto 100%
    background-repeat:no-repeat
    background-position:center

lest-font-size = 0.037rem
less-font-size = 0.04rem
base-font-size = 0.043rem
bigger-font-size = 0.046rem
biggest-font-size = 0.05rem
margin-height = 0.05rem
brand-black = #333
brand-pink = #ff5a89
margin-height-half = 0.025rem
